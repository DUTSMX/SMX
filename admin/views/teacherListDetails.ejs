<body>
<iframe src="/users/navigation" style="width:100vw;height:60px;z-index: 100" frameborder="0" scrolling="no"></iframe>
<div class="main">
    <div class="container">
        <div class="bs-example">
            <div class="panel panel-default">
                <!-- Default panel contents -->
                <div class="panel-heading">
                    <h3 class="panel-title" style="display: inline-block">教师详情</h3>
                </div>
                <div style="margin-left:30px">
                    <h3><%=detail.teacherName%></h3><h5><%=detail.teacherSchool%></h5><h5><%=detail.teacherGoodCourse%></h5>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="main">
    <div class="container">
        <div class="bs-example">
            <div class="panel panel-default">
                <!-- Default panel contents -->
                <div class="panel-heading">
                    <h3 class="panel-title">已有课程</h3>
                </div>
                <!-- Table -->
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                        <tr>
                            <!--<th>编号</th>-->
                            <!--<th>老师</th>-->
                            <th>课程名称</th>
                            <th>开始时间</th>
                            <th>面向对象</th>
                            <!--<th>课程内容</th>-->
                            <th>参与人数</th>
                            <!--<th>操作</th>-->
                        </tr>
                        </thead>
                        <tbody>
                        <%list1.forEach(function (item1){ %>
                        <tr onclick="details(<%=item1.courseId%>)">
                            <!--<td><%=item1.courseId%></td>-->
                            <!--<td><%=item1.teacherName%></td>-->
                            <td><%=item1.courseName%></td>
                            <td><%=item1.beginTime%></td>
                            <td><%=item1.objectOriented%></td>
                            <!--<td><%=item1.courseContent%></td>-->
                            <td><%=item1.courseCount%></td>
                            <!--<td>
                                <a class="btn btn-success" href="courseDetailsEdit?courseId=<%=item1.courseId%>">编辑</a>
                                <a class="btn btn-primary" href="courseDetails?courseId=<%=item1.courseId%>">查看详情</a>
                            </td>-->
                        </tr>
                        <%})%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


