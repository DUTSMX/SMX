<script>
    function student(){
        location.href="joinReceptionStudentList"
    }
    function today(){
        var date = new Date()
        location.href="joinReceptionTodayCourse?date="+date.getFullYear()+"-"+(date.getMonth()+1)+"-"+date.getDate();
    }
    function course(){
        location.href="joinReceptionTeacherList"
    }
    function manager() {
        location.href="joinReceptionCourseManager"
    }
    function postCourse() {
        location.href="joinReceptionCoursePost"
    }
    function checkCourse() {
        location.href="joinReceptionCourseCheck"
    }
    function person(){
        location.href = "joinReceptionDetail"
    }
</script>